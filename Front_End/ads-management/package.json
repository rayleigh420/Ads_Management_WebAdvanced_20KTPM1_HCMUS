    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.0",
    "sass": "^1.69.5",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
